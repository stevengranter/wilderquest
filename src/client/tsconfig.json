{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "module": "ES2020",
        "moduleResolution": "bundler",
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "emitDeclarationOnly": false,
        "declaration": true,
        "baseUrl": ".",
        "paths": {
            "@shared/*": ["../shared/*"],
            "@/*": ["./*"]
        }
    }
}
