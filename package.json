{
    "name": "crudvale",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "lint:server": "biome lint --write src/server",
        "lint:client": "biome lint --write src/client",
        "lint": "pnpm run lint:server && pnpm run lint:client",
        "dev": "NODE_ENV=development nodemon --verbose --watch src/server --ext ts,json --exec tsx src/server/server.ts",
        "start:preview": "NODE_ENV=production tsx src/server/server.ts",
        "start": "NODE_ENV=production node dist/server/server.js",
        "build": "NODE_ENV=production rm -rf dist && vite build && tsc -p src/server/tsconfig.json && tsc-alias -p src/server/tsconfig.json",
        "dev:bg": "nohup pnpm dev > dev.log 2>&1 & echo $! > .dev.pid",
        "stop:dev": "kill $(cat .dev.pid 2>/dev/null || echo '') 2>/dev/null || true && rm -f .dev.pid",
        "start:bg": "nohup pnpm start > start.log 2>&1 & echo $! > .start.pid",
        "stop:start": "kill $(cat .start.pid 2>/dev/null || echo '') 2>/dev/null || true && rm -f .start.pid",
        "stop:all": "pnpm stop:dev && pnpm stop:start",
        "status": "echo '=== Server Status ===' && (test -f .dev.pid && echo 'Dev server: RUNNING (PID:' $(cat .dev.pid) ')' || echo 'Dev server: STOPPED') && (test -f .start.pid && echo 'Prod server: RUNNING (PID:' $(cat .start.pid) ')' || echo 'Prod server: STOPPED') && echo 'Background processes:' && ps aux | grep -E '(node.*server|nodemon)' | grep -v grep | wc -l",
        "clean": "echo '=== Force Cleanup ===' && pkill -f 'node.*server.js' && pkill -f 'nodemon' && pkill -f 'tsx.*server.ts' && rm -f .dev.pid .start.pid && echo 'Cleanup complete'",
        "check-processes": "echo '=== Process Check ===' && echo 'Node server processes:' && ps aux | grep -E '(node.*server|nodemon|tsx.*server)' | grep -v grep | wc -l && echo 'Total background processes:' && ps aux | grep -E '(node|jest|pnpm)' | grep -v grep | wc -l",

        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:server": "jest --testPathPatterns=src/server",
        "test:client": "jest --testPathPatterns=src/client"
    },
